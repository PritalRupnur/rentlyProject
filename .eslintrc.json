{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 200 }],
        "new-cap": 0,
        "require-jsdoc" : 0,
        "valid-jsdoc":0,
        "no-invalid-this":0,
        "no-unused-vars":0,
        "indent":0,
        "no-trailing-spaces":0,
        "no-multiple-empty-lines":0,
        "padded-blocks":0
        



    }
}
